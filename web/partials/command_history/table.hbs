
<div class="block">
    <table class="table">
        <thead>
        <tr>
            <th><span class="has-tooltip-arrow" data-tooltip="Bot Group is group of agents">Group</span></th>
            <th><span class="has-tooltip-arrow" data-tooltip="Bots are agents of a Bot Group">Bot</span></th>
            <th><span class="has-tooltip-arrow" data-tooltip="State Conditions are scored and executed, per Bot">Condition</span></th>
            <th><span class="has-tooltip-arrow" data-tooltip="Time Condition was initiated">Started</span></th>
            <th><span class="has-tooltip-arrow" data-tooltip="Score of the Condition to make it execute">Score</span></th>
        </tr>
        </thead>
        {{#if_command_history_length commandHistory 20}}
            <tfoot>
            <tr>
                <th><span class="has-tooltip-arrow" data-tooltip="Bot Group is group of agents">Group</span></th>
                <th><span class="has-tooltip-arrow" data-tooltip="Bots are agents of a Bot Group">Bot</span></th>
                <th><span class="has-tooltip-arrow" data-tooltip="State Conditions are scored and executed, per Bot">Condition</span></th>
                <th><span class="has-tooltip-arrow" data-tooltip="Time Condition was initiated">Started</span></th>
                <th><span class="has-tooltip-arrow" data-tooltip="Score of the Condition to make it execute">Score</span></th>
            </tr>
            </tfoot>
        {{/if_command_history_length}}
        <tbody>
        </tbody>

        {{#each commandHistory as |command commandIndex|}}
        <tr>
            <td><a href="bot_group?bot_group_id={{command.BotGroupName}}">{{command.BotGroupName}}</a></td>
            <td><a href="bot?bot_id={{command.BotName}}&bot_group_id={{command.BotGroupName}}">{{command.BotName}}</a></td>
            <td>{{command.ConditionName}}</td>
            <td>{{format_time command.Started}}</td>
            <td>{{command.Score}}</td>
        </tr>
        {{/each}}

    </table>
</div>